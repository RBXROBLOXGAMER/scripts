-- Scripts:

local function ZNDHDG_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local button = script.Parent
	local frame = script.Parent.Parent.Parent.Parent.Console
	button.MouseButton1Click:Connect(function()
		frame:Destroy()
	end)
end
coroutine.wrap(ZNDHDG_fake_script)()
local function BWVD_fake_script() -- Write.LocalScript 
	local script = Instance.new('LocalScript', Write)

	local TweenService = game:GetService("TweenService")
	local LogService = game:GetService("LogService")
	
	local scriptParent = script.Parent
	local textLabel = scriptParent.Parent.Write
	local scrollingFrame = scriptParent.Parent.Parent.ScrollingFrame
	
	local currentHeight = 550
	local animationInfo = TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ñ–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏—è —Å–æ–æ–±—â–µ–Ω–∏–π –≤ –∫–æ–Ω—Å–æ–ª–∏
	local function formatMessage(message, messageType)
		local formattedMessage = message
		local textColor = Color3.new(1, 1, 1)
	
		if messageType == Enum.MessageType.MessageError then
			formattedMessage = "> ‚õî " .. message
			textColor = Color3.new(1, 0, 0)
		elseif messageType == Enum.MessageType.MessageWarning then
			formattedMessage = "> ‚ö† " .. message
			textColor = Color3.new(1, 1, 0)
		elseif messageType == Enum.MessageType.MessageInfo then
			formattedMessage = "> üîµ " .. message
			textColor = Color3.new(1, 1, 1)
		elseif messageType == Enum.MessageType.MessageOutput then
			formattedMessage = "> üì§ " .. message
			textColor = Color3.new(1, 1, 1)
		end
	
		return formattedMessage, textColor
	end
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞ –≤ —Ç–µ–∫—Å—Ç–æ–≤–æ–º –ø–æ–ª–µ –∏ –∞–Ω–∏–º–∞—Ü–∏–∏ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–æ–≤ –æ–∫–Ω–∞
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Ç–µ–∫—Å—Ç–∞ –≤ —Ç–µ–∫—Å—Ç–æ–≤–æ–º –ø–æ–ª–µ –∏ –∞–Ω–∏–º–∞—Ü–∏–∏ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–æ–≤ –æ–∫–Ω–∞
	local function updateConsole()
		local consoleText = LogService:GetLogHistory()
		local formattedText = ""
		local totalHeight = 0
	
		for _, value in ipairs(consoleText) do
			if type(value) == "table" and value.message then
				local formattedMessage, textColor = formatMessage(value.message, value.messageType)
				formattedText = formattedText .. formattedMessage .. "\n"
			elseif type(value) == "string" then
				formattedText = formattedText .. tostring(value) .. "\n"
			end
		end
	
		-- –ó–∞–º–µ–Ω—è–µ–º —Ç–µ–∫—É—â–∏–π —Ç–µ–∫—Å—Ç –Ω–∞ –Ω–æ–≤—ã–π —Ñ–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–Ω—ã–π —Ç–µ–∫—Å—Ç
		textLabel.Text = formattedText
	
		-- –ü–µ—Ä–µ—Å—á–∏—Ç—ã–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä—ã –∏ –∞–Ω–∏–º–∏—Ä—É–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏–µ –æ–∫–Ω–∞ –∏ —Å–∫—Ä–æ–ª–ª–∏–Ω–≥–∞
		local textSizeY = textLabel.TextBounds.Y
		local newHeight = math.max(textSizeY, 550) + 15
	
		local textTween = TweenService:Create(scriptParent, animationInfo, { Size = UDim2.new(0, 475, 0, newHeight) })
		textTween:Play()
	
		local scrollingFrameTween = TweenService:Create(scrollingFrame, animationInfo, { CanvasSize = UDim2.new(0, 0, 0, newHeight) })
		scrollingFrameTween:Play()
	
		-- –ü—Ä–æ–∫—Ä—É—á–∏–≤–∞–µ–º –≤–≤–µ—Ä—Ö –ø—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏ –Ω–æ–≤—ã—Ö —Å–æ–æ–±—â–µ–Ω–∏–π
		scrollingFrame.CanvasPosition = Vector2.new(0, newHeight)
	end
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—á–∏—Å—Ç–∫–∏ –∫–æ–Ω—Å–æ–ª–∏
	local function clearConsole()
		LogService:ClearOutput()
		updateConsole() -- –û–±–Ω–æ–≤–ª—è–µ–º –∫–æ–Ω—Å–æ–ª—å –ø–æ—Å–ª–µ –æ—á–∏—Å—Ç–∫–∏
	end
	
	-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–π –æ—á–∏—Å—Ç–∫–∏ –∫–∞–∂–¥—ã–µ 5 –º–∏–Ω—É—Ç (300 —Å–µ–∫—É–Ω–¥)
	local function autoClearConsole()
		clearConsole()
		wait(300) -- –ü–æ–¥–æ–∂–¥–∞—Ç—å 5 –º–∏–Ω—É—Ç –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–µ–π –æ—á–∏—Å—Ç–∫–æ–π
		autoClearConsole() -- –†–µ–∫—É—Ä—Å–∏–≤–Ω—ã–π –≤—ã–∑–æ–≤ –¥–ª—è –ø–æ–≤—Ç–æ—Ä–µ–Ω–∏—è
	end
	
	updateConsole()
	
	-- –ü–æ–¥–∫–ª—é—á–∞–µ–º –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–æ–Ω—Å–æ–ª–∏ –∫ —Å–æ–±—ã—Ç–∏—é –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –Ω–æ–≤–æ–≥–æ —Å–æ–æ–±—â–µ–Ω–∏—è
	LogService.MessageOut:Connect(updateConsole)
end
coroutine.wrap(BWVD_fake_script)()
local function EZVNRQ_fake_script() -- Console.LocalScript 
	local script = Instance.new('LocalScript', Console)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local frame = script.Parent
	frame.Parent.Enabled = true
	local dragging = false
	local dragInput, mousePos, framePos
	
	local function update(input)
		local delta = input.Position - mousePos
		local newPosition = UDim2.new(
			framePos.X.Scale,
			framePos.X.Offset + delta.X,
			framePos.Y.Scale,
			framePos.Y.Offset + delta.Y
		)
	
		TweenService:Create(frame, TweenInfo.new(0.2), {Position = newPosition}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			mousePos = input.Position
			framePos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
	
end
coroutine.wrap(EZVNRQ_fake_script)()
local function SOMRZW_fake_script() -- Console.LocalScript 
	local script = Instance.new('LocalScript', Console)

	local TweenService = game:GetService("TweenService")
	
	local function fadeIn(element)
		element.Visible = true
		local tweenInfo = TweenInfo.new(
			3, -- –í—Ä–µ–º—è –∞–Ω–∏–º–∞—Ü–∏–∏
			Enum.EasingStyle.Quint, -- –°—Ç–∏–ª—å –∞–Ω–∏–º–∞—Ü–∏–∏
			Enum.EasingDirection.Out -- –ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –∞–Ω–∏–º–∞—Ü–∏–∏
		)
		local tween = TweenService:Create(element, tweenInfo, {Transparency = 0.3})
		tween:Play()
	end
	
	local frame = script.Parent.Parent.Console
	frame.Transparency = 1
	fadeIn(frame)
	
end
coroutine.wrap(SOMRZW_fake_script)()
local function EUKY_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		game.LogService:ClearOutput()
		script.Parent.Parent.ScrollingFrame.Write.Text = ''
	end)
	
end
coroutine.wrap(EUKY_fake_script)()
